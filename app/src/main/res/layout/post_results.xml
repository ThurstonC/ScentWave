                    android:textSize="20dp"
                    android:layout_gravity="center"
                    android:background="@android:color/white"
                    android:layout_margin="1dp"
                    />
                <TextView
                    android:id="@+id/rework_bay_19"
                    android:layout_height="fill_parent"
