        </TableLayout>
        <TextView
            android:layout_width="399dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/comment_label"
            android:text="Comments"
            android:gravity="center"
            android:layout_below="@id/rework_table"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <EditText
            android:layout_width="399dp"
            android:layout_height="99dp"
            android:id="@+id/results_comments"
            android:layout_below="@id/comment_label"
            android:gravity="center_horizontal"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            android:textSize="15dp"
            android:inputType="textAutoComplete|textMultiLine|textCapSentences"
            />
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="100dp"
            android:layout_below="@id/results_comments"
            android:background="@android:color/darker_gray"
            >
            <Button
                android:id="@+id/post_results_button"
                android:layout_width="199dp"
                android:layout_height="99dp"
                android:text="Post Results"
                android:textSize="30dp"
                android:onClick="postResults"
                android:gravity="center"
                android:background="@android:color/holo_green_light"
                android:layout_marginRight="1dp"
                android:layout_marginBottom="1dp"
                />
            <Button
                android:id="@+id/cancel_results_button"
                android:layout_width="199dp"
                android:layout_height="99dp"
                android:text="Cancel"
                android:onClick="cancelResults"
                android:textSize="30dp"
                android:gravity="center"
                android:layout_toRightOf="@+id/post_results_button"
                android:background="@android:color/holo_red_light"
                android:layout_marginRight="1dp"
                android:layout_marginBottom="1dp"
                />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
