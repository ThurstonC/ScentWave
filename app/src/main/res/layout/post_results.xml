<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="fill_parent"
    android:layout_width="fill_parent">
    <RelativeLayout
        android:layout_height="450dp"
        android:layout_width="400dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="50dp"
        android:background="@android:color/darker_gray"
        >
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_operator_name_label"
            android:text="Operator Name:"
            android:gravity="left|center_vertical"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_operator_name"
            android:text="Ted"
            android:layout_toRightOf="@id/results_operator_name_label"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_active_bays_label"
            android:text="Active Bays:"
            android:gravity="left|center_vertical"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            android:layout_below="@id/results_operator_name_label"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_active_bays"
            android:text="24"
            android:layout_toRightOf="@id/results_active_bays_label"
            android:layout_below="@id/results_operator_name"
            android:gravity="center"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_passed_label"
            android:text="Passed:"
            android:gravity="left|center_vertical"
            android:layout_below="@id/results_active_bays_label"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_passed"
            android:text="22"
            android:layout_toRightOf="@id/results_active_bays_label"
            android:layout_below="@id/results_active_bays"
            android:gravity="center"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_failed_label"
            android:text="Failed:"
            android:gravity="left|center_vertical"
            android:layout_below="@id/results_passed_label"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_failed"
            android:text="2"
            android:layout_toRightOf="@id/results_failed_label"
            android:layout_below="@id/results_passed"
            android:gravity="center"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_start_time_label"
            android:text="Start Time:"
            android:gravity="left|center_vertical"
            android:layout_below="@id/results_failed_label"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_start_time"
            android:text="2014 04 20 11:37:42"
            android:layout_toRightOf="@id/results_start_time_label"
            android:layout_below="@id/results_failed"
            android:gravity="center"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_stop_time_label"
            android:text="Stop Time:"
            android:gravity="left|center_vertical"
            android:layout_below="@id/results_start_time_label"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_stop_time"
            android:text="2014 04 20 11:59:42"
            android:layout_toRightOf="@id/results_stop_time_label"
            android:layout_below="@id/results_start_time"
            android:gravity="center"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_duration_label"
            android:text="Duration:"
            android:gravity="left|center_vertical"
            android:layout_below="@id/results_stop_time_label"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="199dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/results_duration"
            android:text="00:22:00"
            android:layout_toRightOf="@id/results_duration_label"
            android:layout_below="@id/results_stop_time"
            android:gravity="center"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <TextView
            android:layout_width="399dp"
            android:layout_height="29dp"
            android:textSize="20dp"
            android:id="@+id/comment_label"
            android:text="Comments"
            android:gravity="center"
            android:layout_below="@id/results_duration_label"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            />
        <EditText
            android:layout_width="399dp"
            android:layout_height="99dp"
            android:id="@+id/results_comments"
            android:layout_below="@id/comment_label"
            android:gravity="center_horizontal"
            android:background="@android:color/white"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="1dp"
            android:textSize="15dp"
            android:inputType="textAutoComplete|textMultiLine|textCapSentences"
            />
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="100dp"
            android:layout_below="@id/results_comments"
            android:background="@android:color/darker_gray"
            >
            <Button
                android:id="@+id/post_results_button"
                android:layout_width="199dp"
                android:layout_height="99dp"
                android:text="Post Results"
                android:textSize="30dp"
                android:onClick="postResults"
                android:gravity="center"
                android:background="@android:color/holo_green_light"
                android:layout_marginRight="1dp"
                android:layout_marginBottom="1dp"
                />
            <Button
                android:id="@+id/cancel_results_button"
                android:layout_width="199dp"
                android:layout_height="99dp"
                android:text="Cancel"
                android:onClick="cancelResults"
                android:textSize="30dp"
                android:gravity="center"
                android:layout_toRightOf="@+id/post_results_button"
                android:background="@android:color/holo_red_light"
                android:layout_marginRight="1dp"
                android:layout_marginBottom="1dp"
                />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
